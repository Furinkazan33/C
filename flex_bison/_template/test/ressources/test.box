typedef vehicle(string manufacturer, string model, array components)
typedef motor(string manufacturer, string model)
typedef tire(string manufacturer, int size)


tire michelin = { "Michelin", 42 }
motor ferrari = { "ferrari", "V8turbo" }
string name = "205"
vehicle car = { "Peugeot", name, [michelin, ferrari] }


